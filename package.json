{
    "name": "syki-seo",
    "version": "1.0.0",
    "description": "Site report generator",
    "main": "bin/index.js",
    "files": [
        "!lib/__tests__/**/*",
        "lib/**/*",
        "bin/**/*"
    ],
    "bin": {
        "syki-seo": "bin/index.js"
    },
    "scripts": {
        "start": "ts-node ./src/index.ts",
        "build": "tsc",
        "clean": "rm -rf ./lib/",
        "prepare": "husky install",
        "test:watch": "jest --watch",
        "test": "jest --coverage",
        "typecheck": "tsc --noEmit",
        "lint": "eslint \"./src/**/*.{js,,ts,,json}\"",
        "lint-fix": "eslint --fix \"./src/**/*.{js,,ts,,json}\"",
        "format": "prettier --write \"./src/**/*.{js,ts,json,md}\""
    },
    "license": "MIT",
    "engines": {
        "node": ">=19.0"
    },
    "dependencies": {
        "cheerio": "^1.0.0-rc.12",
        "commander": "^10.0.0",
        "robots-parser": "^3.0.1",
        "sitemapper": "^3.2.6",
        "superagent": "^8.0.9"
    },
    "devDependencies": {
        "@ryansonshine/commitizen": "^4.2.8",
        "@ryansonshine/cz-conventional-changelog": "^3.3.4",
        "@types/jest": "^29.4.0",
        "@types/node": "^18.14.5",
        "@types/superagent": "^4.1.16",
        "@typescript-eslint/eslint-plugin": "^5.54.0",
        "@typescript-eslint/parser": "^5.54.0",
        "eslint": "^8.35.0",
        "eslint-config-prettier": "^8.6.0",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-prettier": "^4.2.1",
        "execa": "^7.0.0",
        "jest": "^29.4.3",
        "lint-staged": "^13.1.2",
        "prettier": "^2.8.4",
        "ts-jest": "^29.0.5",
        "ts-node": "^10.9.1",
        "typescript": "^4.9.5"
    },
    "directories": {
        "lib": "lib"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/xSyki/syki-seo.git"
    },
    "keywords": [
        "seo",
        "tester",
        "site",
        "test",
        "web scraper"
    ],
    "author": "xSyki",
    "types": "./lib/index.d.ts",
    "bugs": {
        "url": "https://github.com/xSyki/syki-seo/issues"
    },
    "homepage": "https://github.com/xSyki/syki-seo#readme"
}
